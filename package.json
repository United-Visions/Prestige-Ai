{
  "name": "prestige-ai",
  "version": "0.1.6",
  "description": "A simplified Claude Code CLI integration inspired by CCdyad",
  "type": "commonjs",
  "main": "dist-electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "start": "node scripts/dev.cjs",
    "test:setup": "node scripts/test-setup.js",
    "postinstall": "node scripts/fix-sqlite3-bindings.js",
    "build": "node scripts/fix-sqlite3-bindings.js && tsc && mv dist-electron/main.js dist-electron/main.cjs 2>/dev/null || true && vite build && electron-builder",
    "build:dir": "node scripts/fix-sqlite3-bindings.js && tsc && vite build && electron-builder --dir",
    "build:mac": "node scripts/fix-sqlite3-bindings.js && tsc && vite build && electron-builder --mac",
    "build:win": "node scripts/fix-sqlite3-bindings.js && tsc && vite build && electron-builder --win",
    "build:linux": "node scripts/fix-sqlite3-bindings.js && tsc && vite build && electron-builder --linux",
    "build:all": "tsc && vite build && electron-builder --mac --win --linux",
    "build:versioned": "node scripts/organize-builds.js --structure-only && npm run build:all && node scripts/organize-builds.js",
    "build:mac:versioned": "node scripts/organize-builds.js --structure-only && npm run build:mac && node scripts/organize-builds.js",
    "build:win:versioned": "node scripts/organize-builds.js --structure-only && npm run build:win && node scripts/organize-builds.js",
    "build:linux:versioned": "node scripts/organize-builds.js --structure-only && npm run build:linux && node scripts/organize-builds.js",
    "build:clean": "node scripts/organize-builds.js --clean",
    "build:organize": "node scripts/organize-builds.js",
    "package:mac": "npm run build:mac",
    "package:win": "npm run build:win",
    "package:all": "npm run build:all",
    "release": "node scripts/build-releases.js",
    "release:mac": "npm run build:mac && node scripts/build-releases.js",
    "release:win": "npm run build:win && node scripts/build-releases.js",
    "serve:downloads": "cd download-site && node server.js",
    "open:downloads": "open http://localhost:3000",
    "deploy:downloads": "cd download-site && npm run deploy",
    "preview": "vite preview",
    "electron": "wait-on tcp:5173 && electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on tcp:5173 && electron .\"",
    "rebuild": "npx electron-rebuild -f -w better-sqlite3",
    "dist": "npm run build && electron-builder",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test": "npm run test:unit && npm run test:component && npm run test:e2e",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:component": "playwright test -c playwright-ct.config.ts",
    "test:component:watch": "playwright test -c playwright-ct.config.ts --ui",
    "test:e2e": "playwright test",
    "test:e2e:watch": "playwright test --ui",
    "test:integration": "playwright test tests/integration/",
    "test:install": "playwright install",
    "test:install:deps": "playwright install-deps"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.12",
    "@ai-sdk/google": "^2.0.12",
    "@ai-sdk/openai": "^2.0.24",
    "@ai-sdk/openai-compatible": "^1.0.15",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/better-sqlite3": "^7.6.13",
    "ai": "^5.0.33",
    "better-sqlite3": "^11.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.6.2",
    "lucide-react": "^0.400.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "stacktrace-js": "^2.0.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.2.2",
    "zod": "^4.1.3",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.55.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.11",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^27.3.11",
    "electron-builder": "^24.6.4",
    "electron-rebuild": "^3.2.9",
    "electron-reload": "^2.0.0-alpha.1",
  "eslint": "^8.45.0",
  "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^26.1.0",
    "mongodb-memory-server": "^10.2.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "vite": "^4.4.5",
    "vite-plugin-electron": "^0.15.5",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "^3.2.4",
    "wait-on": "^7.2.0"
  },
  "build": {
    "afterPack": "./scripts/fix-sqlite3-bindings.js",
    "appId": "com.prestige-ai.app",
    "productName": "Prestige-AI",
    "directories": {
      "output": "builds/v0.1.6"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "extraResources": [
      {
        "from": "assets/",
        "to": "assets/",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "worker/",
        "to": "worker/",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "scaffold/",
        "to": "scaffold/",
        "filter": [
          "**/*"
        ]
      }
    ],
    "publish": {
      "provider": "github",
      "owner": "United-Visions",
      "repo": "Prestige-Ai"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "assets/icon/icon.icns",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "assets/icon/icon.ico"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "tar.gz",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "assets/icon/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": false
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  }
}
